# This tmux statusbar config was created by tmuxline.vim
# on Tue, 13 May 2014

set -g status-bg 'colour235'
set -g message-command-fg 'colour243'
set -g status-justify 'left'
set -g status-left-length '100'
set -g status 'on'
set -g pane-active-border-fg 'colour243'
set -g message-bg 'colour234'
set -g status-right-length '100'
set -g status-right-attr 'none'
set -g message-fg 'colour243'
set -g message-command-bg 'colour234'
set -g status-attr 'none'
# set -g status-utf8 'on'
set -g pane-border-fg 'colour234'
set -g status-left-attr 'none'
setw -g window-status-fg 'colour250'
setw -g window-status-attr 'none'
setw -g window-status-activity-bg 'colour235'
setw -g window-status-activity-attr 'none'
setw -g window-status-activity-fg 'colour243'
setw -g window-status-separator ''
setw -g window-status-bg 'colour235'
set -g status-left '#[fg=colour234,bg=colour243] #S #[fg=colour243,bg=colour235,nobold,nounderscore,noitalics]'
set -g status-right '#[fg=colour234,bg=colour235,nobold,nounderscore,noitalics]#[fg=colour243,bg=colour234] %Y-%m-%d  %H:%M #[fg=colour243,bg=colour234,nobold,nounderscore,noitalics]#[fg=colour234,bg=colour243] #h '
setw -g window-status-format '#[fg=colour250,bg=colour235] #I #[fg=colour250,bg=colour235] #W '
setw -g window-status-current-format '#[fg=colour235,bg=colour234,nobold,nounderscore,noitalics]#[fg=colour243,bg=colour234] #I #[fg=colour243,bg=colour234] #W #[fg=colour234,bg=colour235,nobold,nounderscore,noitalics]'
